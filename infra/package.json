{
  "name": "gen-ai-agent-consensus-infra",
  "version": "0.1.0",
  "config": {
    "deployment_prefix": "genAiAgentConsensus"
  },
  "bin": {
    "main": "bin/main.ts"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "eslint . --ext '**/*.js,.jsx,.ts,.tsx' --ignore-pattern node_modules/",
    "test": "jest",
    "test:coverage": "jest --silent --coverage --verbose",
    "test:build": "run-s format:check lint test:coverage build",
    "watch": "tsc -w",
    "build": "tsc --build -v -f",
    "build:lint": "run-s format:check lint build",
    "bootstrap": "cross-env ts-node bin/cdk-runner.ts bootstrap",
    "synth": "cross-env ts-node bin/cdk-runner.ts synth",
    "deploy": "cross-env WRITE_ENV_FILES=true ts-node bin/cdk-runner.ts deploy",
    "destroy": "cross-env ts-node bin/cdk-runner.ts destroy"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "aws-cdk": "2.1019.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^51.2.3",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.35.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.201.0",
    "constructs": "^10.0.0"
  }
}
