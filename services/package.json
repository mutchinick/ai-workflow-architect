    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^51.2.3",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
